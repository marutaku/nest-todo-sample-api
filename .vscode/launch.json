{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "name": "Launch via yarn",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "start:debug"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "node_modules/**"
      ],
      "sourceMaps": true,
      "envFile": "/workspace.env",
      "cwd": "/workspace/",
      "console": "integratedTerminal",
      "protocol": "inspector"
    },
  ]
}