{
  "name": "nest-todos",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "yarn lint:tsc && yarn lint:eslint && yarn lint:prettier",
    "format": "yarn lint:prettier && yarn format:eslint",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:prettier": "prettier --check src/**/*.ts test/**/*.ts",
    "lint:tsc": "tsc --noEmit",
    "format:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "fix": "yarn format && yarn lint",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d config/typeorm-cli-config.ts",
    "commitmsg": "commitlint -e $HUSKY_GIT_PARAMS",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/apollo": "12.2.1",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "8.4.7",
    "@nestjs/graphql": "10.2.1",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.1.4",
    "apollo-server-express": "3.6.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "graphql": "^16.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "8.13.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "swagger-ui-express": "4.6.3",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.7",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.13",
    "@types/node": "18.19.54",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
